<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" minHeight="700.0" minWidth="500.0" prefHeight="784.0" prefWidth="891.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ua.dscorp.poessence.windows.FilterController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <children>
      <TableView fx:id="tableView" prefHeight="629.0" prefWidth="851.0" />
      <Button fx:id="saveButton" mnemonicParsing="false" onAction="#onSaveButtonClick" text="Done" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button onAction="#onBackButtonClick" mnemonicParsing="false" text="Back">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button onAction="#onSelectButtonClick" mnemonicParsing="false" text="Select all">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button onAction="#onUnselectButtonClick" mnemonicParsing="false" text="Unselect all">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
